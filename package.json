{
  "name": "focuss",
  "description": "FocusS",
  "author": "Mitch Schwartz",
  "version": "1.0.2",
  "private": true,
  "provisioning": "Apple Distribution",
  "dependencies": {
    "assert": "^2.0.0",
    "cra-template": "1.2.0",
    "cryptiles": "^4.1.3",
    "docker": "^1.0.0",
    "electron-is-dev": "^2.0.0",
    "electron-packager": "^16.0.0",
    "focuss": "https://github.com/pragmaflowinc/focuss",
    "fs": "^0.0.1-security",
    "ga-gtag": "^1.1.1",
    "lodash": "^4.17.21",
    "marked": "^4.1.0",
    "node-localstorage": "^2.2.1",
    "npm": "^8.18.0",
    "nth-check": "^2.1.1",
    "os": "^0.1.2",
    "path": "^0.12.7",
    "react": "^18.2.0",
    "react-countdown": "^2.3.2",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1",
    "react-timer-hook": "^3.0.5",
    "ui-neumorphism": "^1.1.3",
    "universal-analytics": "^0.5.3",
    "update-electron-app": "^2.0.1",
    "uuid": "^9.0.0",
    "v8-compile-cache": "^2.3.0",
    "web-vitals": "^2.1.4",
    "yarn": "^1.22.19"
  },
  "main": "./public/electron.js",
  "macStoreOutputPath": "./FocusS-mas-x64/FocusS.app",

  "repository": {
    "type": "git",
    "url": "https://github.com/pragmaflowinc/focuss"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "devLin": "concurrently -k \"BROWSER=none yarn start\" \"yarn:electron\"",
    "dev": "concurrently -k \"react-scripts start\" \"yarn:electron\"",
    "dev:nA": "concurrently -k \"react-scripts start\" \"yarn:electron --noAnalytics\"",
    "electron": "wait-on tcp:3000 && electron .",
    "build:mac": "yarn build && electron-builder -m -c.extraMetadata.main=build/electron.js",
    "build:win": "yarn build && electron-builder -w -c.extraMetadata.main=build/electron.js",
    "build:linux": "yarn build && electron-builder -l -c.extraMetadata.main=build/electron.js",
    "build:macstore": "electron-builder --macos",
    "build:macstore-old": "electron-packager . 'FocusS' --app-version='1.0.1' --build-version='1.0.106' --platform=mas --arch=x64 --app-bundle-id=com.pragmaflow.focuss --helper-bundle-id=com.pragmaflow.focuss --icon='./build/favicon.icns' --ignore='dist/' --overwrite --osx-sign --out ./dist/mas &&  electron-osx-flat './dist/mas/FocusS.app' --verbose",
    "sign:macstore": "electron-osx-sign './dist/mas/FocusS.app' --entitlements=./build/entitlements.plist --verbose",
    "flat:macstore": "electron-osx-flat './dist/mas/FocusS.app' --verbose",
    "validate:mac": "xcrun altool --validate-app --type=macos --file=./dist/mas/FocusS.pkg --apiKey=J848GY4LHX --apiIssuer=748e6f85-abef-4f98-b404-623333d78336",
    "upload:mac": "xcrun altool --upload-app --type=macos --file=./dist/mas/FocusS.pkg --apiKey=J848GY4LHX --apiIssuer=748e6f85-abef-4f98-b404-623333d78336",
    "flbld:mac": "yarn build && yarn electron:build:macstore-old && yarn upload:mac"
  },
  "homepage": ".",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^7.3.0",
    "cross-var": "1.1.0",
    "dotenv-cli": "3.0.0",
    "electron": "^20.0.3",
    "electron-builder": "^23.3.3",
    "wait-on": "^6.0.1"
  },
  "build": {
    "appId": "com.pragmaflow.focuss",
    "productName": "FocusS",
    "buildVersion": "1.0.105",
    "artifactName": "FocusS.pkg",
    "mac": {
      "target": "mas"
    },
    "win": {
      "target": "portable",
      "icon": "./public/favicon.png"
    },
    "nsis": {
      "artifactName": "FocusS"
    },
    "portable": {
      "unicode": false,
      "artifactName": "focuss.exe"
    },
    "target": "node",
    "icon": "./build/favicon.icns"
  }
}
